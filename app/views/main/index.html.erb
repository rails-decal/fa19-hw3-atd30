<% City.all.each do |c| %>
<% if not c.nil? %>
    <h1> <%= c.name %> </h1>
    <p> 
        It's <%= c.temperature %>&#8457 
        and it looks like <%= c.description %> 
    </p>
<% end %>
<% end %>
<%= form_with local: true, method: "get" do |f| %>
    <%= f.label :city %>
    <%= f.text_field :city %>
    <%= f.submit %>
<% end %>